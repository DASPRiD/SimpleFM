{
    "name": "soliantconsulting/simplefm",
    "type": "library",
    "description": "FileMaker Server XML API Adapter",
    "keywords": [
        "FileMaker",
        "adapter"
    ],
    "homepage": "http://github.com/soliantconsulting/SimpleFM",
    "license": "MIT",
    "authors": [
        {"name": "Jeremiah Small", "email": "jsmall@soliantconsulting.com"}
    ],
    "require": {
      "php": ">=5.5.0",
      "doctrine/common": ">=2.2.0"
    },
    "require-dev": {
        "zendframework/zendframework": "2.4.*",
        "phpunit/PHPUnit": "4.7.*",
        "phpunit/phpunit-skeleton-generator": "*",
        "mockery/mockery": "~0.9.4",
        "squizlabs/php_codesniffer": "2.*",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "suggest": {
        "zendframework/zendframework": "Required if you plan on using SimpleFM/ZF2 components."
    },
    "autoload": {
        "psr-0": {
            "Soliant\\": "library/",
            "SoliantTest\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
